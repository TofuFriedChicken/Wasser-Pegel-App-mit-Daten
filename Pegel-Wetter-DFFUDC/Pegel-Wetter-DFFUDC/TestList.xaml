<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Pegel_Wetter_DFFUDC.TestList">

<VerticalStackLayout>

<!-- source ListView: https://learn.microsoft.com/de-de/dotnet/maui/user-interface/controls/listview?view=net-maui-8.0 (last visit: 14.07.24)-->
<ListView x:Name="testList">
    <ListView.ItemTemplate>
        <DataTemplate>
            <Label Text="{Binding measurementData}"/>
        </DataTemplate>
    </ListView.ItemTemplate>
</ListView>


</VerticalStackLayout>
</ContentPage>